#!/bin/sh
nitrogen --restore &
picom > /dev/null &
mpd --no-daemon &> /dev/null &
xsetroot -cursor_name left_ptr &
${HOME}/.config/polybar/launch.sh &
dunstify -t 20000 -a system -i ~/Pictures/arch-dark.png "Hello, AlphaNecron" "Boot duration: $(systemd-analyze time | tail -1 | awk {'print $4'})\nSession: $DESKTOP_SESSION" &
#dunstify -t 20000 -a system -i partition "Partition status" "

